<?xml version="1.0" encoding="UTF-8"?>
<window>
    <controls>
        <control type="image">
            <left>0</left>
            <top>0</top>
            <width>1920</width>
            <height>1080</height>
            <fadetime>500</fadetime>
            <texture>common/black.png</texture>
            <animation type="conditional" condition="Control.HasFocus(5001)|Control.HasFocus(5060)" reversible="true">
                <effect type="fade" start="0" end="75" time="0" />
            </animation>
        </control>
        <control type="group">
            <left>320</left>
            <top>250</top>
            <animation type="WindowOpen" reversible="false">
                <effect type="fade" start="0" end="100" time="200" tween="sine" easing="in" />
                <effect type="zoom" start="50" end="100" time="200" center="910,440" tween="quadratic" easing="out" />
            </animation>
            <animation type="WindowClose" reversible="false">
                <effect type="fade" end="0" start="100" time="200" tween="sine" easing="out" />
                <effect type="zoom" end="75" start="100" time="200" center="910,440" tween="quadratic" easing="in" />
            </animation>
            <control type="group">
                <width>1250</width>
                <control type="image">
                    <height>525</height>
                    <texture colordiffuse="FF4a4347">common/white.png</texture>
                </control>
                <control type="image">
                    <height>100</height>
                    <texture colordiffuse="royalblue">common/white.png</texture>
                </control>
                <control type="image">
                    <left>15</left>
                    <top>15</top>
                    <width>70</width>
                    <height>70</height>
                    <align>left</align>
                    <aligny>top</aligny>
                    <aspectratio aligny="top">keep</aspectratio>
                    <texture colordiffuse="FFCCCCCC">common/fen.png</texture>
                </control>
            </control>
            <control type="image">
                <left>530</left>
                <top>110</top>
                <width>705</width>
                <height>405</height>
                <aspectratio>keep</aspectratio>
                <texture>$INFO[Container(5001).ListItem.Property(tikiskins.window.image)]</texture>
            </control>
            <control type="list" id="5001">
                <pagecontrol>5060</pagecontrol>
                <left>0</left>
                <top>135</top>
                <width>480</width>
                <height>360</height>
                <onup>5001</onup>
                <ondown>5001</ondown>
                <onright>5060</onright>
                <orientation>Vertical</orientation>
                <itemlayout height="90" width="480">
                    <control type="label">
                        <width max="400">auto</width>
                        <left>40</left>
                        <height>90</height>
                        <font>font12</font>
                        <textcolor>FFCCCCCC</textcolor>
                        <align>center</align>
                        <aligny>center</aligny>
                        <label>[UPPERCASE]$INFO[ListItem.Property(tikiskins.window.name)][/UPPERCASE]</label>
                    </control>
                </itemlayout>
                <focusedlayout height="90" width="480">
                    <control type="image">
                        <width>720</width>
                        <height>90</height>
                        <aligny>top</aligny>
                        <texture colordiffuse="FFCCCCCC">common/white.png</texture>
                    </control>
                    <control type="image">
                        <width>30</width>
                        <height>90</height>
                        <aligny>top</aligny>
                        <texture colordiffuse="royalblue">common/white.png</texture>
                    </control>
                    <control type="label">
                        <left>40</left>
                        <width max="400">auto</width>
                        <height>90</height>
                        <font>font12</font>
                        <textcolor>FF4a4347</textcolor>
                        <align>center</align>
                        <aligny>center</aligny>
                        <label>[UPPERCASE]$INFO[ListItem.Property(tikiskins.window.name)][/UPPERCASE]</label>
                    </control>
                </focusedlayout>
            </control>
            <control type="scrollbar" id="5060">
                <top>125</top>
                <left>490</left>
                <width>30</width>
                <height>380</height>
                <texturesliderbackground border="15">scrollbars/scrollbaruni-bg.png</texturesliderbackground>
                <texturesliderbar border="15">scrollbars/scrollbaruni-bar.png</texturesliderbar>
                <texturesliderbarfocus colordiffuse="FF9c9a9b" border="15">scrollbars/scrollbaruni-bar-focus.png</texturesliderbarfocus>
                <textureslidernib>scrollbars/scrollbaruni-nib.png</textureslidernib>
                <textureslidernibfocus>scrollbars/scrollbaruni-nib.png</textureslidernibfocus>
                <onleft>5001</onleft>
                <onright>5001</onright>
                <showonepage>false</showonepage>
                <orientation>vertical</orientation>
                <visible>Control.IsVisible(5001)</visible>
            </control>
        </control>
    </controls>
</window>
